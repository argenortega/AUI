QPushButton{
	background-color: rgb(255, 255, 255);
    border-style: outset;
    border-width: 1px;
    border-radius: 6px;
	min-width: 44px;
	border-color: rgb(193, 193, 193);
	border-style: solid;
	padding: 6px;

}
QPushButton:pressed {
    border-style: solid;
	border-width: 1px;
    border-radius: 6px;
	background-color: rgb(48, 131, 251);
	color: rgb(255, 255, 255);
}

QPushButton:hover{
	border-color: rgb(164, 205, 255);
    border-radius: 6px;
	border-width: 3px;
	border-style: solid;
}